      <Get Name="Get" Id="{ec801c58-aecc-470d-ae31-07982bee2e88}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[FlashTime := tFlashTime;]]></ST>
