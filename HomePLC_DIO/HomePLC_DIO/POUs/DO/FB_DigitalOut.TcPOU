      <Get Name="Get" Id="{00921dad-3c6b-43c0-8d74-4b59645454a6}">
        <Declaration><![CDATA[]]></Declaration>
        <Implementation>
          <ST><![CDATA[HmiData.sName := sName;
HmiData.eState := eState;
HmiData.bValue := obOut;]]></ST>
        </Implementation>
      </Get>
    </Property>
